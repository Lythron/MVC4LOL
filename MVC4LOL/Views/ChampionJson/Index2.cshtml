@{
    ViewBag.Title = "Index2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h2>Index2</h2>
</div>

<script type="text/javascript">
    
    $(document).ready(function () {
        
        startup.init();
        
    });

</script>
