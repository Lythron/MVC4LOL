<div id="Patches">

</div>
<div style="clear:both" ></div>
<div id="Tags">

</div>
<div style="clear:both" ></div>
<div id="Champions" >
    
</div>
<div style="clear:both" ></div>
<div id="ChampionsDetails" >
    <table id="ChampionDetailsTable" >
        <tr>
            <th>
                pic
            </th>
            <th>
                name
            </th>
            <th>
                health
            </th>
            <th>
                @*<label title="Healh per level">hpl</label>*@
                hpl
            </th>
            <th>
                hreg
            </th>
            <th>
                hrpl
            </th>
            <th>
                armor
            </th>
